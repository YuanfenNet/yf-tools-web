{
  "name": "yf-tools-web",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "preview": "nuxt preview",
    "generate": "nuxt generate",
    "postinstall": "nuxt prepare",
    "version": "git add . && git commit -m \"v$npm_package_version\"",
    "postversion": "git tag $npm_package_version && git push && git push --tags",
    "deploy": "pnpm version --no-git-tag-version",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install",
    "commit": "git-cz"
  },
  "dependencies": {
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/language": "^6.10.1",
    "@lezer/highlight": "^1.2.0",
    "@sentry/browser": "^7.114.0",
    "@sentry/vue": "^7.114.0",
    "@vueuse/core": "^10.9.0",
    "@vueuse/nuxt": "^10.9.0",
    "codemirror": "^6.0.1",
    "crypto-js": "^4.2.0",
    "js-base64": "^3.7.7",
    "jsonlint-mod": "^1.7.6",
    "jspdf": "^2.5.1",
    "luxon": "^3.4.4",
    "mediainfo.js": "^0.2.2",
    "sortablejs": "^1.15.2",
    "sortablejs-vue3": "^1.2.11",
    "vue-codemirror": "^6.1.1",
    "vue-drag-zone": "2.0.0-rc1",
    "vue-json-viewer": "^3.0.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.17.0",
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@commitlint/cz-commitlint": "^18.6.1",
    "@element-plus/nuxt": "^1.0.9",
    "@nuxt/content": "^2.12.1",
    "@nuxt/devtools": "latest",
    "@nuxt/eslint": "^0.3.12",
    "@nuxtjs/color-mode": "^3.4.1",
    "@nuxtjs/sitemap": "^5.1.5",
    "@sentry/vite-plugin": "^2.16.1",
    "@types/crypto-js": "^4.2.2",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.12.11",
    "@types/sortablejs": "^1.15.8",
    "commitizen": "^4.3.0",
    "element-plus": "^2.7.2",
    "eslint": "^8.57.0",
    "eslint-plugin-format": "^0.1.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "nuxt": "^3.11.2",
    "nuxt-gtag": "^2.0.5",
    "sass": "^1.77.0",
    "sass-loader": "^14.2.1",
    "vite-plugin-static-copy": "^1.0.4"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "lint-staged": {
    "*.{vue,js,ts}": "eslint --fix"
  }
}
